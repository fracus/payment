<h1>Lista de Categorias</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Precio Hs</th>
  </tr>

  <% @categories.each do |category| %>
    <tr>
      <td><%= category.name %></td>
      <td><%= category.price_hs %></td>
      <td><%= link_to 'Show', category %></td>
      <td><%= link_to 'Edit', edit_category_path(category) %></td>
            
    </tr>

  <% end %>
</table>