<p>
  <strong>Editar Categoria</strong>
</p>

<%= form_for(@category) do |f| %>
      
      <p>
     <%= f.label :name%>
      <%= f.text_field :name, class: 'form-control' %>
     </p>
     <p>
      <%= f.label :price_hs %>
      <%= f.text_field  :price_hs, class: 'form-control' %>
    </p>
      
      <%= f.submit "Save changes", class: "btn btn-primary" %>
      
    <% end %>
 
